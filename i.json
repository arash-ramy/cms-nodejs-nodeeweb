{
  "@context": "https://schema.org",
  "@type": "BlogPosting",
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": "${obj.url}"
  },
  "headline": "${obj.title}",
  "description": "${obj.description}",
  "image": "${obj.image}",
  "datePublished": "${obj.createdAt}",
  "dateModified": "${obj.updatedAt}"
}